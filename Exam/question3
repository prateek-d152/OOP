#include <iostream>
using namespace std;

class Calculator {
public:
    int add(int a, int b) {
        return a + b;
    }
    float add(float a, float b) {
        return a + b;
    }
};

int main() {
    Calculator calc;
    int intResult = calc.add(5, 10);
    float floatResult = calc.add(5.5f, 10.2f);
    cout << "Integer addition result: " << intResult << endl;
    cout << "Float addition result: " << floatResult << endl;
    return 0;
}
